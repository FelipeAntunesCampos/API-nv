generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Comidas {
    id        Int      @id @default(autoincrement())
    nome      String
    descricao String 
    preco     Float
    categoria String 
    avaliacao Float
    createdAt DateTime @default(now())

    @@map("comidas")
}